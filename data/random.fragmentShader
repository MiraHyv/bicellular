#version 330 core

//from image:
in vec2 UV;
uniform sampler2D tx;
uniform vec2[25] texelOffsets;

//from convolution filter:
uniform float[25] convolutionMatrix;
uniform float convolutionLevel;
uniform float dryLevel;

out vec3 outColor;


void main() {
    float newValue;
    outColor = vec3(newValue, newValue, newValue);
}
